echo "all varibales passed: $@"
echo "number of variables passed: $#"
echo "script name: $0"
echo "present working directory: $PWD"
echo "home directory of working user: $HOME"
echo "which user is executing this script: $USER"
echo "process id of current script: $$"
ecgo "process id of last executed command: $!"
echo "last executed command exit status: $?"
echo "shell name: $SHELL"
echo "operating system type: $OSTYPE"
echo "current line number in script: $LINENO"
echo "last executed command: $BASH_COMMAND"
echo "bash version: $BASH_VERSION"